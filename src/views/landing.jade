h3 you've landed!
input(type='text' ng-model='handle')
p {{ lowercasehandle() }}

.maths
  p MATHS
  input(type="text" placeholder="enter a number" ng-model="main.num")
  div(ng-show='main.num % 2 === 0')
    p number is divisible by 2
  div(ng-show='main.num % 3 === 0')
    p number is divisible by 3
  div(ng-show='main.num % 4 === 0')
    p number is divisible by 4
  div(ng-show='main.num % 5 === 0')
    p number is divisible by 5
  div(ng-show='main.num % 6 === 0')
    p number is divisible by 6
  div(ng-show='main.num % 7 === 0')
    p number is divisible by 7
  div(ng-show='main.num % 8 === 0')
    p number is divisible by 8
  div(ng-show='main.num % 9 === 0')
    p number is divisible by 9

.cards
  button(ng-click="getDeck()") Get Deck
  p {{ deckId }}
  .decks
    ul
      li(ng-repeat="resp in deckArr")
        p {{ resp }}
